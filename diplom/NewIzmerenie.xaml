               Width="86"
               Margin="9,213,0,0"
               HorizontalAlignment="Left"
               VerticalAlignment="Top"
               Content="Имя пробы"
               RenderTransformOrigin="0.5,0.5">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform />
                    <SkewTransform AngleX="-2.045" />
                    <RotateTransform />
                    <TranslateTransform X="-0.464" />
                </TransformGroup>

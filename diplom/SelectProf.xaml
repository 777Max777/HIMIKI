<Window x:Class="diplom.SelectProf"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:diplom"
        mc:Ignorable="d"
        Title="Выбор профиля" Height="300" Width="300">
    <Grid>
        <ComboBox x:Name="comboBox" HorizontalAlignment="Left" Margin="95,41,0,0" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label" Content="Выберите профиль" HorizontalAlignment="Left" Margin="95,10,0,0" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="button" Content="Далее" HorizontalAlignment="Left" Margin="192,225,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>

    </Grid>
</Window>
